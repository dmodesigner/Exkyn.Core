<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>

    <PackageId>Exkyn.Core</PackageId>
    <Version>7.0.4</Version>
    <Authors>Daniel Moura de Oliveira</Authors>
    <Company>Exkyn Tecnologia</Company>
    <RepositoryUrl>https://github.com/dmodesigner/Exkyn.Core</RepositoryUrl>
    <Title>Exkyn Core</Title>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageIcon>icon-exkyn.png</PackageIcon>
    <PackageTags>Exkyn; Exkyn.Core</PackageTags>
    <Description>Biblioteca com m√©todos genericos para uso dentro de projetos .NET</Description>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\Images\icon-exkyn.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNet.WebApi.Client" Version="5.2.9" />
  </ItemGroup>

</Project>
