<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>

    <PackageId>exkyn.core</PackageId>
    <Version>7.0.1</Version>
    <Authors>Daniel Moura de Oliveira</Authors>
    <Company>Exkyn Tecnologia</Company>
    <RepositoryUrl>https://github.com/dmodesigner/Exkyn.Core</RepositoryUrl>
    <GeneratePachageOnBuild>true</GeneratePachageOnBuild>
    <Title>Exkyn Core</Title>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageIcon>icon-exkyn.jpg</PackageIcon>
    <PackageTags>Exkyn Core</PackageTags>
    <Description>Biblioteca com m√©todos genericos para uso dentro de projetos .NET
</Description>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\Exkyn\_Designer\Logomarca\icon-exkyn.jpg">
